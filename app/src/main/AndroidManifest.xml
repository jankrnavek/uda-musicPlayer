<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JazzActivity"
            android:label="Jazz songs library"
            android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".RockActivity"
            android:label="Rock songs library"
            android:parentActivityName=".MainActivity"></activity>

        <activity
            android:name=".ReggaeActivity"
            android:label="Reggae songs library"
            android:parentActivityName=".ReggaeActivity"></activity>

        <activity
            android:name=".FunkyActivity"
            android:label="Funky songs library"
            android:parentActivityName=".FunkyActivity"></activity>

    </application>

</manifest>